! fonts:

Xft.antialias:          true
Xft.rgba:               rgb

URxvt*font:             xft:Inconsolata \LGC:size=12:style=Thin, xft:PowerlineSymbols:size=12
URxvt.allow_bold:       false
URxvt*letterSpace:      -1

! settings:

URxvt.scrollBar:        false
URxvt.urgentOnBell:     false
URxvt*borderless:       1
URxvt.internalBorder:   3
URxvt.lineSpace:        0
URxvt.iso14755:         false
URxvt.iso14755_52:      false

! plugins

URxvt.perl-ext-common:  default,selection-to-clipboard

!colors:

! special
*.foreground:           #e0e7ee
*.background:           #354e6a
URxvt.background:       rgba:1111/1111/1111/dddd
*.cursorColor:          #e0e7ee

! black
*.color0:               #383c3d
*.color8:               #272323

! red
*.color1:               #bc8787
*.color9:               #bb5d5d

! green
*.color2:               #7e9b7e
*.color10:              #588c58

! yellow
*.color3:               #c8c88f
*.color11:              #d3c571

! blue
*.color4:               #a5a5ce
*.color12:              #8585d2

! magenta
*.color5:               #bb95bb
*.color13:              #c379c3

! cyan
*.color6:               #a1c8c8
*.color14:              #73c0c0

! white
*.color7:               #ded3d3
*.color15:              #f7f5f5

